<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >

        <Button
            android:id="@+id/btn_simple_banner"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Simple Banner"
            android:textColor="@color/white"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_marginTop="@dimen/_10sdp"
            />

        <Button
            android:id="@+id/btn_native_banner"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Native Banner"
            android:textColor="@color/white"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_marginTop="@dimen/_10sdp"
            />

        <Button
            android:id="@+id/btn_preload_inter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Preloaded Interstitial"
            android:textColor="@color/white"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_gravity="center"
            android:gravity="center"/>

        <Button
            android:id="@+id/btn_simple_inter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Simple Interstitial"
            android:textColor="@color/white"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_gravity="center"
            android:gravity="center"/>

    </LinearLayout>
</layout>
